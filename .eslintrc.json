{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "camelcase": "warn",
        "no-console": "warn",
        "semi": "warn",
        "require-jsdoc": "off"
    }
}
